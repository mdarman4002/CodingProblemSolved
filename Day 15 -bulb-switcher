class Solution {
    public int bulbSwitch(int n) {
        // int count = 0 ;
        // int i = 1 ;

        // while(i*i<=n){
        //     count++;
        //     i++;
        // }
        // return count ;
        int on = 0; 
        for(int i = 1; i * i <= n; i++){
            on++;
            // if( i * i <= n){
            //     on++;
            // }
        }
        return on;
    }
}
