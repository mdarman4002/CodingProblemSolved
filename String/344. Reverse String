class Solution {
    public void reverseString(char[] s) {
        int left = 0;
        int right = s.length-1;
        while(left < right){
            // char ch = s.charAt(left);
            // s.charAt(left) = s.charAt(right);
            // s.charAt(right) = ch;
            char ch = s[left];
            s[left] = s[right];
            s[right] = ch;
            left++;
            right--; 
            
        }
    }
}
