class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int len = m + n;
        int index = m;
        for(int i = 0; i < n; i++){
            nums1[index] = nums2[i];
            index += 1;
        }
        Arrays.sort(nums1);
        System.out.print(Arrays.toString(nums1));
    }
}
